# Uppgift 3
# Hitta det största talet i en lista



def max_in_list(numbers: list) -> int:
    
    highest = numbers[0]  #Start nummer
    for num in numbers:
        if num > highest:  #Kollar ifall ett tal är > än det nuvarande högsta talet
            highest = num  #Högsta talet uppdateras
    return highest


print(max_in_list([1, 2, 3]))  
print(max_in_list([-5, 0, 5]))
print(max_in_list([10]))
